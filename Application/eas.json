{
  "cli": {
    "version": ">= 10.1.1",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "apk"
      }
    },
    "release": {
      "android": {
        "buildType": "apk",
        "distribution": "store",
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
